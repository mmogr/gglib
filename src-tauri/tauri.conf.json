{
  "productName": "GGLib GUI",
  "version": "0.3.3",
  "identifier": "com.gglib.gui",
  "build": {
    "beforeBuildCommand": "npm run build:tauri",
    "beforeDevCommand": "npm run dev",
    "frontendDist": "../web_ui"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "GGLib - GGUF Model Manager",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "default"
      ]
    }
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "Copyright \u00a9 2025 mmogr",
    "externalBin": [],
    "icon": [
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon.png"
    ],
    "resources": [],
    "shortDescription": "",
    "macOS": {
      "entitlements": "Entitlements.plist",
      "minimumSystemVersion": "10.13"
    }
  }
}
